    "version": "0.2.0",
    "configurations": [    
        {
            "name": "Python: Attach using Process Id",
            "type": "python",
            "request": "attach",
            "processId": "${command:pickProcess}",
            "justMyCode": true
        },
        {
            "name": "Python: Module",
            "type": "python",
            "request": "launch",
            "module": "src.jroutes.serve",
            "args": [
                "-r",
                "tests.routes"
            ],
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "/home/debian/tpalko/.virtualenv/jroutes/lib/python3.9/site-packages:/media/floor/development/github.com/jroutes:/media/floor/development/github.com/jroutes/src"
            }
        }
    ]
}